{
    $schema: 'https://docs.renovatebot.com/renovate-schema.json',
    extends: ['config:base'],
    ignoreDeps: [],
    packageRules: [
        {
            groupName: 'Node.js',
            matchPackageNames: ['node'],
            matchPackagePatterns: ['/node$'],
            excludePackageNames: ['calico/node', 'kindest/node', '@sentry/node'],
        },    
    ],
}
